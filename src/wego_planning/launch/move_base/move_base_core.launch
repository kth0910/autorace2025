<launch>
  <node pkg="move_base" type="move_base" name="move_base" output="screen">
    <rosparam file="$(find wego_planning)/config/move_base/costmap_common.yaml" command="load"/>
    <rosparam file="$(find wego_planning)/config/move_base/global_costmap.yaml" command="load"/>
    <rosparam file="$(find wego_planning)/config/move_base/local_costmap.yaml" command="load"/>
    <rosparam file="$(find wego_planning)/config/move_base/dwa_local_planner.yaml" command="load"/>
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS"/>
  </node>
</launch>

